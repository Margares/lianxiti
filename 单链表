#include <stdio.h>
#include<stdlib.h>
typedef struct pe{
	int num;
	int sa;
	struct pe * next;
}per;
per *creat (int n){//创建新链表
	per *head,*p;
	int i;
	head=(per *)malloc (sizeof (per));
	head=NULL;
	p=head;
	for(i=0;i<n;i++){
		scanf("%d %d",&p->num,&p->sa);
		p->next=new(per);
		p->next->next=NULL;
		p=p->next;
	}
	p->next=NULL;
	return(head);
}
int list( per *p){//输出链表 
	printf("%d %d",p->num,p->sa);
	return 0;
}
int main (){
	int n,i;
	per *p1;
	scanf ("%d",&n);
	p1=creat(n);
	while(p1!=NULL){
		list(p1++);
	}
return 0;
}

//仍然有问题，只能输入一行
